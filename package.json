{
  "name": "pkg-debug",
  "description": "Tool for debug npm package with replace version from npm and local development versions",
  "version": "0.0.1",
  "repository": "http://github.com/lexich/pkg-debug",
  "main": "index.js",
  "bin": {
    "pkgDebug": "cli.js"
  },
  "license": "MIT",
  "scripts": {
    "test": "npm run eslint",
    "eslint": "node_modules/.bin/eslint *.js src/{,**}/*.js src/{,**}/*.jsx",
    "precommit": "npm test",
    "prepush": "npm test",
    "postmerge": "npm install"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "fs-extra": "^0.30.0",
    "inquirer": "^1.2.2"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "csscomb": "3.1.7",
    "eslint": "3.5.0",
    "eslint-config-airbnb": "11.1.0",
    "eslint-plugin-react": "6.3.0",
    "eslint-plugin-import": "1.15.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "husky": "0.11.8",
    "stylelint-config-suitcss": "8.0.0"
  }
}
